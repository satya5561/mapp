<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.mahendra" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mahendra</name>
    <description>
       satya
    </description>
    <author email="satya5561@gmail.com" href="http://ionicframework.com">
      S P Gupta
    </author>
	
	<!-- Define app icon for each platform. -->
    <icon src="icon.png" />
    <icon src="res/icon/android/icon-48-mdpi.png"   gap:platform="android"    gap:density="ldpi" />
    <icon src="res/icon/android/icon-48-mdpi.png"   gap:platform="android"    gap:density="mdpi" />
    <icon src="res/icon/android/icon-48-mdpi.png"   gap:platform="android"    gap:density="hdpi" />
    <icon src="res/icon/android/icon-48-mdpi.png"  gap:platform="android"    gap:density="xhdpi" />
	 <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.contacts" version="0.2.9" />
    <gap:plugin name="org.apache.cordova.geolocation" version="0.3.7" />
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
